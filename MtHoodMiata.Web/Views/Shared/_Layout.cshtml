<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta name="google-site-verification" content="wr3Kvgqaa-O68J55Eye7k7jZ5m_t4MLCnVxu5WZbiHs" />
    <link rel="Shortcut Icon" type="image/x-icon" href="@Url.Content( "~/favicon.ico" )" />
    <link rel="Stylesheet" type="text/css" href="@Url.Content( "~/Content/MtHoodMiata.css" )" />
    <link rel="Stylesheet" type="text/css" href="@Url.Content( "~/Content/themes/ui-darkness/jquery-ui-1.10.0.custom.min.css" )" />
    <script src="@Url.Content( "~/Scripts/jquery-1.9.0.min.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/jquery-ui-1.10.0.min.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/jquery.validate.js" )" type="text/javascript"></script>
    <script src="@Url.Content( "~/Scripts/jquery.validate.unobtrusive.min.js" )" type="text/javascript"></script>
    <style type="text/css">
        body
        {
            background-image: url(/images/PageBackground.gif);
            background-repeat: no-repeat;
        }
    </style>
    <script type="text/javascript">
        $(document).ready(init);

        function init() {
            $("button").button();
        }
    </script>
    <script type="text/javascript">
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-25521936-1']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
    </script>
</head>
<body>
    <img src="/images/CarShow.gif" style="height: 83px; width: 211px; top: 20px; position: absolute"
        alt="Logo" />
    <div style="width: 165px; top: 120px; position: absolute">
        @Html.Partial( @ViewBag.Menu as string )
        @RenderSection( "MenuFooter", false )
    </div>
    @RenderBody()
    <div style="top: 15px; left: 500px; position: absolute">
        <!-- AddThis Button BEGIN -->
        <a class="addthis_button" href="http://www.addthis.com/bookmark.php?v=250&amp;username=xa-4d5fde6434375fd4">
            <img src="http://s7.addthis.com/static/btn/v2/lg-share-en.gif" width="125" height="16"
                alt="Bookmark and Share" style="border: 0" /></a>
        <script type="text/javascript" src="http://s7.addthis.com/js/250/addthis_widget.js#username=xa-4d5fde6434375fd4"></script>
        <!-- AddThis Button END -->
    </div>
